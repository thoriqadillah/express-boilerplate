{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express backend typescript boilerplate",
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register",
    "ext": "ts, js"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && resolve-tspaths",
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "start": "node dist/index.js",
    "dev:swc": "swc ./src -d dist --strip-leading-paths -w",
    "dev:chokidar": "chokidar \"dist/**/*.js\" --command \"kill-port --port 3000 && clear && node dist/index.js\" --silent",
    "dev": "rimraf dist && swc ./src -d dist --strip-leading-paths && run-p dev:*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/client-ses": "^3.529.1",
    "@aws-sdk/client-sns": "^3.521.0",
    "@google-cloud/pubsub": "^4.3.2",
    "@libsql/kysely-libsql": "^0.3.0",
    "@sinclair/typebox": "^0.32.14",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bullmq": "^5.7.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "googleapis": "^133.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.2",
    "mime-db": "^1.52.0",
    "moment": "^2.30.1",
    "mongoose": "^8.3.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.10",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.11",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-db": "^1.43.5",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.11",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "chokidar-cli": "^3.0.0",
    "clear-cli": "^2.0.0",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "kysely-codegen": "^0.11.0",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "resolve-tspaths": "^0.8.18",
    "rimraf": "^5.0.5",
    "run-p": "^0.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
